@import "../handy"

fonts = "HelveticaNeue", "Helvetica Neue", "Helvetica", "Arial", sans-serif

*
	-webkit-text-size-adjust 100%

body
	margin 0px
	padding 0px
	font 16px fonts

h1
	margin 0px
	
h2
	margin 0px
	
h3
	margin 0px
	
a, a img
	border none
	&:hover, &:hover img
		border none

.anchor
	display table

.page
	position relative
	width 960px
	margin auto
	padding-left 20px
	padding-right 20px

@import index
@import one
@import two
